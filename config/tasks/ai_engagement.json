{
  "session_id": "ai_engagement",
  "name": "AI领域互动任务",
  "description": "专注于AI、机器学习相关内容的互动",
  "actions": [
    {
      "action_type": "like",
      "count": 30,
      "min_interval": 2.0,
      "max_interval": 6.0,
      "enabled": true,
      "comment_templates": [],
      "follow_back_ratio": 0.3
    },
    {
      "action_type": "follow",
      "count": 10,
      "min_interval": 8.0,
      "max_interval": 15.0,
      "enabled": true,
      "comment_templates": [],
      "follow_back_ratio": 0.3
    },
    {
      "action_type": "comment",
      "count": 5,
      "min_interval": 20.0,
      "max_interval": 40.0,
      "enabled": true,
      "comment_templates": [
        "Fascinating insights on AI! 🤖",
        "Great perspective on machine learning 📊",
        "This aligns with current AI trends 🔥",
        "Thanks for sharing this AI research! 🙏",
        "Interesting approach to solving AI challenges 💡",
        "The future of AI looks promising with ideas like this ✨"
      ],
      "follow_back_ratio": 0.3
    }
  ],
  "target": {
    "keywords": [
      "artificial intelligence",
      "machine learning", 
      "deep learning",
      "neural networks",
      "AI research",
      "ChatGPT",
      "OpenAI",
      "LLM"
    ],
    "hashtags": [
      "#AI",
      "#MachineLearning",
      "#DeepLearning", 
      "#NeuralNetworks",
      "#AIResearch",
      "#DataScience",
      "#ML",
      "#ArtificialIntelligence"
    ],
    "users": [],
    "languages": ["en", "zh"],
    "min_likes": 5,
    "max_age_hours": 24,
    "exclude_keywords": [
      "spam",
      "scam",
      "crypto pump",
      "get rich quick"
    ]
  },
  "max_duration_minutes": 45,
  "max_total_actions": 50,
  "randomize_intervals": true,
  "respect_rate_limits": true
} 